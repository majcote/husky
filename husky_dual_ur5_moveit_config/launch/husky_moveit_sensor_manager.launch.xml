<launch>
    <arg name="sensors_config" default="sensors_xtion_depth_map"/>

    <rosparam command="load" file="$(find husky_dual_ur5_moveit_config)/config/$(arg sensors_config).yaml" />
    <param name="octomap_frame" type="string" value="base_footprint"/>
    <param name="octomap_resolution" type="double" value="0.5"/>
    <param name="max_range" type="double" value="5.0"/>
</launch>
